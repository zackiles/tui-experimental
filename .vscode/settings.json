{
  "deno.enable": true,
  "deno.lint": true,
  "deno.config": "./deno.json",
  "deno.importMap": "./deno.json",
  "deno.unstable": [
    "kv",
    "net",
    "http",
    "fs",
    "ffi",
    "cron",
    "temporal",
    "webgpu",
    "unsafe-proto",
    "webgpu"
  ],
  "deno.codeLens.implementations": true,
  "deno.codeLens.references": true,
  "deno.codeLens.referencesAllFunctions": true,
  "deno.codeLens.test": true,
  "deno.internalDebug": true,
  "deno.internalInspect": true,
  "deno.codeLens.testArgs": ["-A"],
  "deno.internalInspect": true,
  "deno.maxTsServerMemory": 6000,
  "files.eol": "\n",
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "editor.detectIndentation": false,
  "editor.indentSize": 2,
  "editor.insertSpaces": true,
  "typescript.tsserver.experimental.enableProjectDiagnostics": false,
  "workbench.editorAssociations": {
    "*.mdc": "default",
    "*.copilotmd": "vscode.markdown.preview.editor",
    "*.md": "vscode.markdown.preview.editor"
  },
  "files.associations": {
    "repomix.config.json": "json5"
  },
  "[json][jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[markdown][typescript][javascript]": {
    "editor.defaultFormatter": "denoland.vscode-deno",
    "editor.formatOnSave": true
  },
  "search.exclude": {
    "snapshot.xml": true
  },
  "cSpell.dictionaries": [
    "!typescript"
  ],
  "cSpell.words": [
    "agentic",
    "alicloud",
    "asynciterable",
    "Blitter",
    "Blitters",
    "COLORTERM",
    "datacontenttype",
    "dataschema",
    "dbus",
    "dconf",
    "deno",
    "denoland",
    "dockerenv",
    "dotenv",
    "DYLD",
    "dylib",
    "FOSS",
    "fseventsd",
    "hiberfil",
    "Iles",
    "inprocess",
    "jsxs",
    "keyrings",
    "libexample",
    "libnotcurses",
    "MILLIS",
    "mlterm",
    "modelcontextprotocol",
    "msvc",
    "ncpile",
    "ncplane",
    "ncvisual",
    "notcurses",
    "onclose",
    "parentbased",
    "pids",
    "putc",
    "puttext",
    "repomix",
    "rwxr",
    "sixel",
    "somevalue",
    "specversion",
    "swapfile",
    "traceparent",
    "tracestate",
    "typeof",
    "unvalidated",
    "USERPROFILE",
    "valibot",
    "vdom",
    "webgpu",
    "zacharyiles",
    "zackiles",
    "Zipkin"
  ]
}
